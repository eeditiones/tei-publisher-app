            <choice>
               <oneOrMore>
                  <group>
                     <ref name="item"/>
                     <zeroOrMore>
                        <ref name="model.global"/>
                     </zeroOrMore>
                  </group>
               </oneOrMore>
               <group>
                  <oneOrMore>
                     <group>
                        <ref name="label"/>
                        <zeroOrMore>
                           <ref name="model.global"/>
                        </zeroOrMore>
                        <ref name="item"/>
                        <zeroOrMore>
                           <ref name="model.global"/>
