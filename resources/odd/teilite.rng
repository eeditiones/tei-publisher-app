   <define name="model.correspDescPart">
      <choice>
         <ref name="note"/>
         <ref name="noteGrp"/>
         <ref name="correspAction"/>
         <ref name="correspContext"/>
      </choice>
   </define>
   <define name="model.resource">
      <choice>
         <ref name="text"/>
         <ref name="standOff"/>
         <ref name="facsimile"/>
      </choice>
   </define>
   <define name="model.objectLike">
      <choice>
         <ref name="object"/>
