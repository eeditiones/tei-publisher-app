        <encodingDesc>
            <tagsDecl>
                <pb:behaviour ident="code" output="latex">
                    <desc>Code listing</desc>
                    <pb:param name="language" value="if (@language = ('xml', 'html', 'sh', 'bash')) then '[language=' || @language || ']' else ()" />
