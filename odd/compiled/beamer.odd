                <macroSpec rend="add" module="tei" type="dt" ident="data.certainty">
                    <content>
                        <choice xmlns="http://relaxng.org/ns/structure/1.0">
                            <value>high</value>
                            <value>medium</value>
                            <value>low</value>
                            <value>unknown</value>
                        </choice>
                    </content>
