                            <param name="maxInclusive">1</param>
                        </data>
                    </content>
                </macroSpec>
                <macroSpec rend="add" module="tei" type="dt" ident="data.numeric">
                    <content>
                        <choice xmlns="http://relaxng.org/ns/structure/1.0">
                            <data type="double"/>
                            <data type="token">
                                <param name="pattern">(\-?[\d]+/\-?[\d]+)</param>
                            </data>
