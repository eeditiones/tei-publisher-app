                    </content>
                </macroSpec>
                <macroSpec rend="add" module="tei" type="dt" ident="data.xTruthValue">
                    <content>
                        <choice xmlns="http://relaxng.org/ns/structure/1.0">
                            <data type="boolean"/>
                            <value>unknown</value>
                            <value>inapplicable</value>
                        </choice>
                    </content>
                </macroSpec>
                <macroSpec rend="add" module="tei" type="dt" ident="data.language">
                    <content>
                        <choice xmlns="http://relaxng.org/ns/structure/1.0">
                            <data type="language"/>
                            <value/>
                        </choice>
                    </content>
                </macroSpec>
                <macroSpec rend="add" module="tei" type="dt" ident="data.namespace">
                    <content>
                        <data xmlns="http://relaxng.org/ns/structure/1.0" type="anyURI"/>
                    </content>
                </macroSpec>
                <macroSpec rend="add" module="tei" type="dt" ident="data.outputMeasurement">
                    <content>
                        <data xmlns="http://relaxng.org/ns/structure/1.0" type="token">
                            <param name="pattern">[\-+]?\d+(\.\d+)?(%|cm|mm|in|pt|pc|px|em|ex|gd|rem|vw|vh|vm)</param>
                        </data>
                    </content>
                </macroSpec>
                <macroSpec rend="add" module="tei" type="dt" ident="data.pattern">
                    <content>
                        <data xmlns="http://relaxng.org/ns/structure/1.0" type="token"/>
                    </content>
                </macroSpec>
                <macroSpec rend="add" module="tei" type="dt" ident="data.point">
                    <content>
                        <data xmlns="http://relaxng.org/ns/structure/1.0" type="token">
                            <param name="pattern">(\-?[0-9]+\.?[0-9]*,\-?[0-9]+\.?[0-9]*)</param>
                        </data>
                    </content>
                </macroSpec>
                <macroSpec rend="add" module="tei" type="dt" ident="data.pointer">
                    <content>
                        <data xmlns="http://relaxng.org/ns/structure/1.0" type="anyURI"/>
                    </content>
                </macroSpec>
                <macroSpec rend="add" module="tei" type="dt" ident="data.version">
                    <content>
                        <data xmlns="http://relaxng.org/ns/structure/1.0" type="token">
                            <param name="pattern">[\d]+(\.[\d]+){0,2}</param>
                        </data>
                    </content>
                </macroSpec>
                <macroSpec rend="add" module="tei" type="dt" ident="data.versionNumber">
                    <content>
                        <data xmlns="http://relaxng.org/ns/structure/1.0" type="token">
                            <param name="pattern">[\d]+[a-z]*[\d]*(\.[\d]+[a-z]*[\d]*){0,3}</param>
                        </data>
                    </content>
                </macroSpec>
                <macroSpec rend="add" module="tei" type="dt" ident="data.replacement">
                    <content>
                        <text xmlns="http://relaxng.org/ns/structure/1.0"/>
                    </content>
                </macroSpec>
