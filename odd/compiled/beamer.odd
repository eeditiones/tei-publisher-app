                                <tei:valItem ident="simple:literal"/>
                                <tei:valItem ident="simple:normalstyle"/>
                                <tei:valItem ident="simple:normalweight"/>
                                <tei:valItem ident="simple:right"/>
                                <tei:valItem ident="simple:rightbraced"/>
                                <tei:valItem ident="simple:rotateleft"/>
                                <tei:valItem ident="simple:rotateright"/>
                                <tei:valItem ident="simple:smallcaps"/>
