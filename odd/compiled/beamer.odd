                                <ref xmlns="http://relaxng.org/ns/structure/1.0" name="data.temporal.w3c"/>
                            </tei:datatype>
                        </tei:attDef>
                    </tei:attList>
                </classSpec>
                <classSpec rend="add" module="tei" type="atts" xml:id="DATABLE" ident="att.datable">
                    <tei:classes xmlns:tei="http://www.tei-c.org/ns/1.0">
                        <memberOf key="att.datable.w3c"/>
                    </tei:classes>
                    <tei:attList xmlns:tei="http://www.tei-c.org/ns/1.0">
                        <tei:attDef ident="calendar" usage="opt">
                            <tei:datatype>
                                <ref xmlns="http://relaxng.org/ns/structure/1.0" name="data.pointer"/>
                            </tei:datatype>
                            <tei:constraintSpec ident="calendar" scheme="isoschematron">
                                <tei:constraint>
                                    <sch:rule xmlns:sch="http://purl.oclc.org/dsdl/schematron" context="tei:*[@calendar]">
                                        <sch:assert test="string-length(.) gt 0">
@calendar indicates the system or calendar to which the date represented by the content of this element
belongs, but this <sch:name/> element has no textual content.</sch:assert>
                                    </sch:rule>
                                </tei:constraint>
                            </tei:constraintSpec>
                        </tei:attDef>
                        <tei:attDef ident="period" usage="opt">
                            <tei:datatype>
                                <ref xmlns="http://relaxng.org/ns/structure/1.0" name="data.pointer"/>
                            </tei:datatype>
                        </tei:attDef>
                    </tei:attList>
                </classSpec>
