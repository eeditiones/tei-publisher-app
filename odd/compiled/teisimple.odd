                            <group xmlns="http://relaxng.org/ns/structure/1.0">
                                <zeroOrMore>
                                    <choice>
                                        <ref name="model.frontPart"/>
                                        <ref name="model.pLike.front"/>
                                        <ref name="model.pLike"/>
                                        <ref name="model.listLike"/>
                                        <ref name="model.global"/>
