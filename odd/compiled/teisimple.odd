                            <egXML xmlns="http://www.tei-c.org/ns/Examples" source="#TwN">
                                <phr>
                                    <c>M</c>
                                    <c>O</c>
                                    <c>A</c>
                                    <c>I</c>
                                    <w>doth</w>
                                    <w>sway</w>
                                    <w>my</w>
                                    <w>life</w>
                                </phr>
                            </egXML>
