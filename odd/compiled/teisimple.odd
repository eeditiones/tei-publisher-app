                        <attList/>
                        <model xmlns:XSL="http://www.w3.org/1999/XSL/Transform" predicate="parent::choice and count(parent::*/*) gt 1" behaviour="inline"/>
                        <model xmlns:XSL="http://www.w3.org/1999/XSL/Transform" behaviour="inline">
